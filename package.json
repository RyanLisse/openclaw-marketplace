{
  "name": "openclaw-marketplace",
  "version": "0.1.0",
  "description": "Intent-based marketplace for OpenClaw agents",
  "private": true,
  "packageManager": "pnpm@10.28.2",
  "scripts": {
    "dev": "pnpm --filter frontend dev",
    "build": "pnpm -r build",
    "start": "pnpm --filter frontend start",
    "lint": "pnpm -r lint",
    "test": "pnpm --filter frontend test",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "prose": "prose run workflows/marketplace-development.prose",
    "openclaw": "pnpm --filter @openclaw/cli exec node dist/index.js",
    "prepare": "husky"
  },
  "devDependencies": {
    "@beads/bd": "^0.49.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "openclaw",
    "marketplace",
    "ai-agents",
    "intent-matching",
    "base-l2",
    "web3"
  ],
  "author": "Ryan Lisse",
  "license": "MIT",
  "dependencies": {
    "convex": "^1.31.7",
    "ethers": "^6.16.0",
    "openai": "^6.17.0"
  }
}
